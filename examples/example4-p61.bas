5 REM THIS PROGRAM INVERTS AN N-BY-N HILBERT MATRIX
10 DIM A(20,20),B(20,20)
20 READ N
30 MAT A = CON(N,N)
50 FOR I = 1 TO N
60 FOR J = 1 TO N
70 LET A(I,J) = 1/(I+J-1)
80 NEXT J
90 NEXT I
100 MAT B = INV(A)
115 PRINT "INV(A) =",
120 MAT PRINT B;
125 PRINT
130 PRINT "DETERMINANT OF A =" DET
190 DATA 4
199 END
